@using SFA.DAS.FAT.Web.Infrastructure
@using SFA.DAS.FAT.Web.Models
@{
    ViewData["Title"] = "Page not found";
    ViewData[nameof(BreadCrumbViewModel)] = new BreadCrumbViewModel {Level = BreadCrumbLevel.Error};
}
<main class="govuk-main-wrapper " id="main-content">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <h1 class="govuk-heading-xl">Page not found</h1>
            <p class="govuk-body">If you entered a web address, check that it's correct. Or you can <a asp-route="@RouteNames.ServiceStart" class="govuk-link govuk-link--no-visited-state">return to the homepage</a>.</p>
        </div>
    </div>
</main>