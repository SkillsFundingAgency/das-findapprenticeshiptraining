@using System.Web
@using Microsoft.AspNetCore.Http
@using SFA.DAS.FAT.Web.Models

<script>
    @if (ViewBag.GaData == null)
    {
        ViewBag.GaData = new GaData();
    }
    window.dataLayer = window.dataLayer || [];
    dataLayer.push({
        'location': '@ViewBag.GaData.Location',
        'providerPositionInList': '@ViewBag.GaData.ProviderPlacement',
        'totalProviders':'@ViewBag.GaData.ProviderTotal'
    });
</script>