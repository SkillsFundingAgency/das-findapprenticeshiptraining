        <div class="govuk-header__content">
            <a asp-route="@RouteNames.ServiceStartDefault" class="govuk-header__link govuk-header__service-name">Find apprenticeship training for your apprentice</a>
        </div>
    </div>
</header>

<div class="govuk-phase-banner govuk-width-container">
    <p class="govuk-phase-banner__content">
        <strong class="govuk-tag govuk-phase-banner__content__tag">
            Beta
        </strong>
        <span class="govuk-phase-banner__text">
            This is a new service – your <a class="govuk-link" href="https://www.smartsurvey.co.uk/s/1ZRQ3HFAT/">feedback</a> will help us to improve it.
        </span>
    </p>
</div>

<div class="govuk-width-container">
    @if (breadCrumbViewModel != null && breadCrumbViewModel.Level > BreadCrumbLevel.Home)
    {
        <div class="govuk-grid-row">
            <div class="govuk-grid-column-three-quarters">
                <div class="govuk-breadcrumbs">
                    <ol class="govuk-breadcrumbs__list">
                        @if (breadCrumbViewModel.Level > BreadCrumbLevel.Home)
                        {
                            <li class="govuk-breadcrumbs__list-item">
                                <a id="home-breadcrumb" class="govuk-breadcrumbs__link" asp-route="@RouteNames.ServiceStart">Home</a>
                            </li>
                        }
                        @if (breadCrumbViewModel.Level > BreadCrumbLevel.CourseList)
                        {
                            <li class="govuk-breadcrumbs__list-item">
                                <a id="courses-breadcrumb" class="govuk-breadcrumbs__link" asp-route="@RouteNames.Courses" asp-route-location="@breadCrumbViewModel.Location">Apprenticeship training courses</a>
                            </li>
                        }
                        @if (breadCrumbViewModel.Level > BreadCrumbLevel.CourseDetails)
                        {
                            <li class="govuk-breadcrumbs__list-item">
                                <a id="course-detail-breadcrumb" class="govuk-breadcrumbs__link" asp-route="@RouteNames.CourseDetails" asp-route-id="@breadCrumbViewModel.CourseId" asp-route-location="@breadCrumbViewModel.Location">@breadCrumbViewModel.CourseDescription</a>
                            </li>
                        }
                        @if (breadCrumbViewModel.Level > BreadCrumbLevel.CourseProvidersList)
                        {
                            <li class="govuk-breadcrumbs__list-item">
                                <a id="providers-breadcrumb" class="govuk-breadcrumbs__link" asp-route="@RouteNames.CourseProviders" asp-all-route-data="@breadCrumbViewModel.ProvidersFilters">Training providers</a>
                            </li>
                        }
                    </ol>
                </div>
            </div>
            @if (breadCrumbViewModel.Level > BreadCrumbLevel.Accessibility)
            {
                <div class="govuk-grid-column-one-quarter">
                    <a id="header-view-shortlist" asp-route="@RouteNames.ShortList" class="govuk-body govuk-link govuk-link--no-visited-state app-view-shortlist-link">View shortlist <span class="app-view-shortlist-link__number" aria-hidden="true">@(ViewData["ShortlistItemCount"] ?? 0)</span></a>    
                </div>
            }            
        </div>
    }
    @RenderBody()

    @if (breadCrumbViewModel != null && breadCrumbViewModel.Level > BreadCrumbLevel.Accessibility) {
        <div class="govuk-body app-shortlist-banner">
            <a class="govuk-link govuk-link--no-visited-state app-back-to-top-link" href="#top">
                <svg class="app-back-to-top-link__icon" xmlns="http://www.w3.org/2000/svg" width="13" height="17" viewBox="0 0 13 17">
                    <path fill="currentColor" d="M6.5 0L0 6.5 1.4 8l4-4v12.7h2V4l4.3 4L13 6.4z"></path>
                </svg>
                Back to top
            </a>
            <a asp-route="@RouteNames.ShortList" class="govuk-link govuk-link--no-visited-state app-view-shortlist-link">View shortlist <span class="app-view-shortlist-link__number" aria-hidden="true">@(ViewData["ShortlistItemCount"] ?? 0)</span></a>
        </div>
    }

</div>

<footer class="govuk-footer">
    <div class="govuk-width-container ">
        <div class="govuk-footer__meta">
            <div class="govuk-footer__meta-item govuk-footer__meta-item--grow">
                <h2 class="govuk-visually-hidden">Support links</h2>
                <ul class="govuk-footer__inline-list">

                    <li class="govuk-footer__inline-list-item">
                        <a class="govuk-footer__link" href="https://help.apprenticeships.education.gov.uk/hc/en-gb" target="_blank" rel="noreferrer noopener">
                            Help
                        </a>
                    </li>

                    <li class="govuk-footer__inline-list-item">
                        <a class="govuk-footer__link" href="https://www.smartsurvey.co.uk/s/1ZRQ3HFAT/" target="_blank" rel="noreferrer noopener">
                            Feedback
                        </a>
                    </li>

                    <li class="govuk-footer__inline-list-item">
                        <a class="govuk-footer__link" asp-route="@RouteNames.Privacy">
                            Privacy
                        </a>
                    </li>

                    <li class="govuk-footer__inline-list-item">
                        <a class="govuk-footer__link" asp-route="@RouteNames.AccessibilityStatement">
                            Accessibility statement
                        </a>
                    </li>

                    <li class="govuk-footer__inline-list-item">
                        <a class="govuk-footer__link" asp-route="@RouteNames.Cookies">
                            Cookies
                        </a>
                    </li>

                    <li class="govuk-footer__inline-list-item">
                        Built by the
                        <a class="govuk-footer__link" href="https://www.gov.uk/government/organisations/education-and-skills-funding-agency" target="_blank" rel="noreferrer noopener">
                            Education and Skills Funding Agency
                        </a>
                    </li>

                </ul>

                <svg aria-hidden="true" focusable="false" class="govuk-footer__licence-logo" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 483.2 195.7" height="17" width="41">
                    <path fill="currentColor" d="M421.5 142.8V.1l-50.7 32.3v161.1h112.4v-50.7zm-122.3-9.6A47.12 47.12 0 0 1 221 97.8c0-26 21.1-47.1 47.1-47.1 16.7 0 31.4 8.7 39.7 21.8l42.7-27.2A97.63 97.63 0 0 0 268.1 0c-36.5 0-68.3 20.1-85.1 49.7A98 98 0 0 0 97.8 0C43.9 0 0 43.9 0 97.8s43.9 97.8 97.8 97.8c36.5 0 68.3-20.1 85.1-49.7a97.76 97.76 0 0 0 149.6 25.4l19.4 22.2h3v-87.8h-80l24.3 27.5zM97.8 145c-26 0-47.1-21.1-47.1-47.1s21.1-47.1 47.1-47.1 47.2 21 47.2 47S123.8 145 97.8 145"></path>
                </svg>
                <span class="govuk-footer__licence-description">
